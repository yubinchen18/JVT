{% extends 'base.html.twig' %}

{% block body %}
    <h1>Phonenumber</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ phonenumber.id }}</td>
            </tr>
            <tr>
                <th>Typelabel</th>
                <td>{{ phonenumber.typeLabel }}</td>
            </tr>
            <tr>
                <th>Digits</th>
                <td>{{ phonenumber.digits }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('phonenumbers_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('phonenumbers_edit', { 'id': phonenumber.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
